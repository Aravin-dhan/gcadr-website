{
  "name": "gcadr-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "content:build": "./scripts/build-static-content.sh",
    "content:dev": "cd backend && python manage.py runserver",
    "content:import": "cd backend && python manage.py import_markdown_blogs && python manage.py import_team_data",
    "content:export": "cd backend && python manage.py export_static_content",
    "deploy": "npm run content:build && git add . && git commit -m 'Update static content' && git push"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@portabletext/react": "^3.0.0",
    "@sanity/client": "^6.8.0",
    "@sanity/image-url": "^1.0.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "clsx": "^2.0.0",
    "date-fns": "^2.30.0",
    "embla-carousel-react": "^8.0.0",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.400.0",
    "next": "^15.5.0",
    "nodemailer": "^6.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.4.0",
    "react-intersection-observer": "^9.5.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.32.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.0",
    "@types/node": "^20.8.0",
    "@types/nodemailer": "^6.4.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.52.0",
    "eslint-config-next": "15.0.3",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0"
  }
}
